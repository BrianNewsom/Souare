{% extends 'MusicMBundle::layout.html.twig' %}

{% block title %}Add Song{% endblock%}

{% block body %}
    <header>
        <h1>Create a song starting with "{{Track.name}}"</h1>
    </header>
    <form action="{{ path('MusicMBundle_addSongFromTrack', {'id': Track.id}) }}" method="post" {{ form_enctype(form) }} class="track">
        {{ form_errors(form) }}

        {{ form_row(form.title) }}
        

        {{ form_rest(form) }}

        <input type="submit" value="Submit" />
    </form>

{% endblock %}