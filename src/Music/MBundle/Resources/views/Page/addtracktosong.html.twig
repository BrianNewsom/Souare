{% extends 'MusicMBundle::layout.html.twig' %}

{% block title %}Add Song{% endblock%}

{% block body %}
    <header>
        <h1>Upload your own Song</h1>
    </header>

    <form action="{{ path('MusicMBundle_addTrackToSong', {'id':Track.id}) }}" method="post" {{ form_enctype(form) }} class="blogger">
        {{ form_errors(form) }}

        {{ form_row(form.select) }}
        

        {{ form_rest(form) }}

        <input type="submit" value="Submit" />
    </form>

{% endblock %}